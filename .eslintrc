{
    "extends": "airbnb-base",
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "rules": {
        "semi": "off",
        "arrow-parens": "off",
        "require-jsdoc": "off",
        "eol-last": "off",
        "operator-linebreak": "off",
        "no-multi-spaces": "off"
    }
}

